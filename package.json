{
    "name": "crimson.js",
    "description": "library for tetr.io",
    "version": "0.0.1-dev",
    "author": "RedStone576",

    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",

    "scripts": {
